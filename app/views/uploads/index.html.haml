%h1 Listing uploads

%table
  %thead
    %tr
      %th File
      %th
      %th
      %th

  %tbody
    - @uploads.each do |upload|
      %tr
        %td= upload.file
        %td= link_to 'Show', upload, class: 'btn btn-primary'
        %td= link_to 'Edit', edit_upload_path(upload), class: 'btn btn-warning'
        %td= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger'

%br

= link_to 'New Upload', new_upload_path, class: 'btn btn-primary'
